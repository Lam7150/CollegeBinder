<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#dbebcd"
    tools:context="com.example.lam7150.collegebinder.MainActivity">

    <Button
        android:id="@+id/button_send"
        android:layout_width="127dp"
        android:layout_height="44dp"
        android:layout_marginTop="310dp"
        android:fontFamily="@font/helveticabold"
        android:onClick="sendMessage"
        android:text="GET STARTED"
        android:textColor="@color/cardview_light_background"
        android:background="#dc000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:backgroundTint="@android:color/black" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="290dp"
        android:layout_height="44dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/helveticabold"
        android:text="@string/collegebinder"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/intro_text"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="80dp"
        android:fontFamily="@font/eb_garamond"
        android:text="@string/intro"
        android:textAlignment="center"
        android:textSize="15sp"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/shoutouts"
        android:layout_width="350dp"
        android:layout_height="65dp"
        android:layout_marginTop="170dp"
        android:fontFamily="@font/eb_garamond"
        android:textAlignment="center"
        android:textSize="12sp"
        android:textColor="#000000"
        android:layout_marginBottom="16dp"
        android:text="@string/citation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</android.support.constraint.ConstraintLayout>
