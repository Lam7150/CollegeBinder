<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="CollegeName">
        <item name="android:textColor">#000000</item>
        <item name="android:textStyle"> bold </item>
    </style>

    <style name="TableHeading" parent="CollegeName">
        <item name="android:textSize"> 20sp </item>
        <item name="android:background">#FF40F281</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">0.002</item>
        <item name="android:layout_column">0</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:background">#000000</item>
    </style>

    <style name="Cell">
        <item name="android:fontFamily">@font/arialbold </item>
        <item name="android:capitalize">characters</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_column">1</item>
        <item name="android:layout_weight">0.499</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="Whitespace">
        <item name="android:layout_height">50dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_column">1</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">#000000</item>
    </style>

</resources>


